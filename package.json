  "dependencies": {
    "ask-sdk": "^2.7.0",
    "ask-sdk-core": "^2.7.0",
    "ask-sdk-model": "^1.24.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.14",
    "typescript": "^3.7.2"
  }
